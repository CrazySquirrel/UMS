{
  "name": "ums",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "build:prod": "ng build --prod && npm run sw",
    "static-serve": "cd dist && live-server --port=7990 --host=localhost --entry-file=/index.html",
    "commit": "git-cz -a",
    "release": "standard-version && git push origin master --tags"
  },
  "pre-commit": [
    "lint",
    "test",
    "e2e",
    "build:prod"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.1",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular-in-memory-web-api": "^0.3.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "live-server": "^1.2.0",
    "material-design-icons": "^3.0.1",
    "pre-commit": "^1.2.2",
    "rxjs": "^5.4.1",
    "sw-precache": "^5.2.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.3",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "commitizen": "^2.9.6",
    "jasmine-core": "~2.6.2",
    "jasmine-expect": "^3.7.0",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-jasmine-matchers": "^3.7.0",
    "protractor": "~5.1.2",
    "standard-version": "^4.2.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
